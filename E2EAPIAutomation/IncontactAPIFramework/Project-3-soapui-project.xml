import java.util.*;
import groovy.json.JsonSlurper;
import java.io.*;
import groovy.json.JsonOutput
import groovy.io.FileType

// Globals 
configSheet ='Config'
driverSheet ='Driver'
apiDetailsSheet ='APIDToCallDetails'
assertionSheet ='Assertion'
emailConfig= 'EmailConfig'
projDir = context.expand('${projectDir}');
excelPath = projDir+'\\TestDriverExcel\\TestDriverExcel.xlsx';



util = new excelUtils(excelPath); // instance of the excelUtil class
asrtObj = new AssertionsUtil(projDir);// instance of the assertion class
emailReport =new EmailReport(); // instance of the emailreport class

// @ testCasesDetails: Map&lt;String,Map&lt;String,String>> contains all the information related the active testcases from the TestDriverExcel.xlsx 

testCasesDetails= util.getTestCasesDetails(util.getActiveTestCases(driverSheet), util.getHeaders(apiDetailsSheet), apiDetailsSheet)



//********************************************************************************************************************************************** 
def itr = testCasesDetails.keySet().iterator();

//Loop that will iterate for all the active testcases.
while(itr.hasNext())
 {
	def testCaseID = itr.next();
	def testCaseDetails =testCasesDetails.get(testCaseID);
	def requestType = testCaseDetails.get('TypeOfRequest');	
	log.info "Making " +requestType +" Request"			 
	switch (requestType.toUpperCase()) {
		case 'GET':
		     runRequest(testCaseDetails,'REST_GET_Request'); // request for GET
			break;
	     case 'POST':
               runRequest(testCaseDetails,'REST_POST_Request'); // request for POST
               break; 
	     case 'DELETE':
               runRequest(testCaseDetails,'REST_DELETE_Request'); // request for DELETE
               break;
	     case 'PUT' :
               runRequest(testCaseDetails,'REST_PUT_Request'); // request for PUT
	          break;
		}			
 }
emailReport.sendReport(util.getSheetDetails(util.getHeaders(emailConfig), emailConfig),ExtentReportsDemo.reportLocation);

//**********************************************************************************************************************************************
/**
     * Makes an API request based on the data provided on the TestDriverExcel.xlsx , 
     * validates the response and sends email to the concerend stakeholders.
     * 
     * @param testCaseDetails
     *            HashMap containing all the information for the active testcases
     * @param requestType
     *            Type of Request
 */
 
public void runRequest(def testCaseDetails, def requestType)
{
	setHost(testCaseDetails);
	setRequestBody(testCaseDetails);
	def prj = testRunner.testCase.testSuite.project.workspace.getProjectByName("InContact_API_Test")
	def tCase = prj.testSuites['Request_Suite'].testCases['RequestTestStep']
	def tStep = tCase.getTestStepByName(requestType)
     tStep.run(testRunner, context)
	//def response = context.expand( '${#[Request_Suite#RequestTestStep#'+requestType+']#Response}' )
     def harResponse = context.expand( '${#[Request_Suite#RequestTestStep#'+requestType+']#HarResponse}' )	
	saveResponse(harResponse,testCaseDetails);
	asrtObj.validateTest(testCaseDetails.get('TestCaseID'),projDir)	
}

//**********************************************************************************************************************************************

/**
     * Set the host URL for the API request to be made.
     * 
     * @param testCaseDetails
     *            HashMap containing all the information for the active testcases
*/
public void setHost(def testCaseDetails)
{
	def host=testCaseDetails.get('BaseUrl');
	def resource =testCaseDetails.get('URIToAppend');
	context['host']=host;
	context['resource']=resource;
}
//**********************************************************************************************************************************************

/**
     * Attaches the request file to the body of the request. Currently it attaches the whole JSON file or XML file based 
     * on the configuration in the TestDriverExcel.xlsx
     * 
     * @param testCaseDetails
     *            HashMap containing all the information for the active testcases
*/
public void setRequestBody(def testCaseDetails)
{
	def filePath;
	if(verifyChainedRequest(testCaseDetails))
	{
		def testCaseID=testCaseDetails.get('ChainResponseTestCaseID');
		def fileExt=util.getCellValue(util.getCloumnNumber('ResponseType', apiDetailsSheet),util.getRowNumber(testCaseID, apiDetailsSheet), apiDetailsSheet);
		filePath = projDir+'\\ResponseFiles\\'+testCaseDetails.get('ChainResponseTestCaseID')+'_Response.'+fileExt;
	}
	else
	{
	  filePath =	projDir +'\\RequestFiles\\'+testCaseDetails.get('TestCaseID')+'_RequestBody.'+testCaseDetails.get('XML/JSONBodyToAttach')
	}
	attachRequest(filePath)
}

//**********************************************************************************************************************************************

/**
     * Verifies whether the TestCase has any dependency on the other testCases.
     * 
     * @param testCaseDetails
     *            HashMap containing all the information for the active testcases
     * @return {boolean}        
*/
public boolean verifyChainedRequest(def testCaseDetails)
{
	if(testCaseDetails.get('ChainResponseTestCaseID')!='') return true;
	else return false;

}

//**********************************************************************************************************************************************
/**
     * Attaches the request file to the body of the request. Currently it attaches the whole JSON file or XML file based 
     * on the configuration in the TestDriverExcel.xlsx
     * 
     * @param filePath
     *            Path of the file from where the request needs to be fetched
     * @param requestType
     *            Type of Request
     * @throws IOException
*/

public void attachRequest(def filePath) throws IOException
{
	def file = new File(filePath);
	def requestBody;
	if(filePath==null)
	{
		log.info "Invalid FilePath";
	}
	else if(filePath.toLowerCase().contains('json'))
	{
         	requestBody = new JsonSlurper().parse(file);
		requestBody=JsonOutput.toJson(requestBody)
//		log.info requestBody
	}
	else if(filePath.toLowerCase().contains('xml'))
	{
		requestBody = new XmlSlurper().parse(file)
//		log.info requestBody
	}
	context['requestBody']=requestBody;
}

//**********************************************************************************************************************************************
/**
     * Saves the response of the request.
     * 
     * @param response
     *            The response from the request
     * @param testCaseDetails
     *            HashMap containing the details related to testCase.
 */

public void saveResponse(def response,def testCaseDetails)
{
	def filePath = projDir+'\\ResponseFiles\\'+testCaseDetails.get('TestCaseID')+'_Response.'+testCaseDetails.get('ResponseType');	
	def File = new PrintWriter (filePath)
	File.println(response)
	File.flush()
	File.close()
	log.info "Response Saved at "+filePath
}
